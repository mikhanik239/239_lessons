import java.util.Scanner;
public class Main {
    public static Scanner in = new Scanner(System.in); // инициализация модуля ввода
    public static void main(String[] args) {
        double a = in.nextDouble(); //вводим исходные данные
        double b = in.nextDouble();
        double c = in.nextDouble();
        double d = in.nextDouble();
        double e = in.nextDouble();
        double f = in.nextDouble();
        System.out.printf("%.2f %.2f\n", c-a, d-b); //по разнице показаний находим расход
        System.out.printf("%.2f %.2f\n", (c-a)*e, (d-b)*f); //зная расход множим его на цену получаем стоимости
        System.out.printf("%.2f", (c-a)*e+(d-b)*f); // суммируем их
    }
}
